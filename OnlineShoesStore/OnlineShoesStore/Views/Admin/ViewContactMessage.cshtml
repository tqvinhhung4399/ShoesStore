
@{
    ViewData["Title"] = "ViewContactMessage";
    Layout = "_Admin";
}


<!-- Start Banner Area -->
<section class="banner-area organic-breadcrumb">
    <div class="container">
        <div class="breadcrumb-banner d-flex flex-wrap align-items-center justify-content-end">
            <div class="col-first">
                <h1>Brand Manager</h1>
                <nav class="d-flex align-items-center">
                    <a href="#">Admin<span class="lnr lnr-arrow-right"></span></a>
                    <a href="#">Contact Manager</a>
                </nav>
            </div>
        </div>
    </div>
</section>
<!-- End Banner Area -->
<!--================Tracking Box Area =================-->
<section class="tracking_box_area section_gap">
    <div class="container">
        <center>
            <h1 style="color:red;">Administrator - Contact Manager</h1>
            @if(ViewBag.Message != null)
            {
                <h3>@ViewBag.Message</h3>
            }
            <a href="/Admin/ContactManager" class="primary-btn">Back to Contact Manager</a>
        </center>
    </div>
</section>
<!--================End Tracking Box Area =================-->
